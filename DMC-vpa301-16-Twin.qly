func Clip_String(strIn as string,nochar as integer)returns string
    dim strLen as integer
    dim strTrimmed as String

'Calculate string length
    strLen = len(strIn)
'Separate off last 2 characters
    strTrimmed = left(strIn,strLen-nochar)

    return strTrimmed

endfunc

