func determine_pump_state(pump_idx as integer) returns integer
    REM Function to determine what state each pump is in. Look at various factors to determine which state we are in.
    REM 0 - Not Cycled
    REM 1 - Cycled, parameters are ok
    REM 2 - Running (Pressure or flow)
    REM 3 - Update Available
    REM 4 - Ramping in pressure mode paused
    REM 5 - Ramping to set pressure
    REM 6 - Cycling cylinder
    REM 7 - Stopping
    REM 8 - Cycled, but invalid start parameters.
    REM 9 - Zeroing transducer

    'if control_pressed[pump_idx]==start then
    '    if fstates[pump_idx]<>0 then
    '        REM We requested start and are now flowing.
    '        control_pressed[pump_idx]=none
    '    else
    '        return 2
    '    endif
    if tmr_zero_xdcr.enabled then
        REM We are waiting for Galil to finish xeroing the transducer.
        return 9
    endif
    if control_pressed[pump_idx]==stop then
        if fstates[pump_idx]==0 then
            REM User requested stop and we have stopped
            control_pressed[pump_idx]=none
        else
            return 7
        endif
    endif

    'elseif control_pressed[pump_idx]==cycle then
    '    if cycldones[pump_idx]==1 then
    '        REM We have finished cycling
    '        control_pressed[pump_idx]=none
    '    else
    '        return 6
    '    endif
    'endif
    if cycling[pump_idx] then
        return 6
    endif

    if fstates[pump_idx]==0 and pstates[pump_idx]==0 then
        REM Cylinder is stopped
        if cycldones[pump_idx]==false then
            return 0
        endif
        if valid_start_params(pump_idx) then
            return 1
        else
            return 8
        endif

    else
        REM Cylinders are moving.
        REM Verify Ramphold, Valuechanged, and rampcomplete are set correctly

        if pstates[pump_idx]<>0 and not currentlyramping[pump_idx] then
            return 4
        elseif valueschanged[pump_idx] then
            return 3
        elseif currentlyramping[pump_idx] then
            return 5
        else
            return 2
        endif

    endif
endfunc

func valid_start_params(pump_idx as integer) returns boolean
    dim drop_down_box_mode as dropdownlistv2
    dim target_eb, rate_eb as editboxv2
    dim float_target, float_rate as float

    if pump_idx==0 then
        REM Set which box to check for mode.
        drop_down_box_mode=ddl_mode_a
        target_eb=eb_target_a
        rate_eb=eb_rate_a

    else
        drop_down_box_mode=ddl_mode_b
        target_eb=eb_target_b
        rate_eb=eb_rate_b
    endif

    check error
        val(float_rate, rate_eb.Value)
    on error
        _clearexception()
        float_rate=-1
    enderr

    if drop_down_box_mode.selected==0 then
        REM Flow mode
        if target_eb.Value=="Inf" and float_rate>0 and float_rate<=frmax then
            return true
        endif

        check error
            val(float_target, target_eb.value)
        on error
            _clearexception()
            float_target=-1
        enderr

        if target_eb.value=="0" and float_rate>0 and float_rate<=frmax then
            target_eb.Value="Inf"
            return true
        endif
        if float_target>0 and float_rate>0 and float_rate<=frmax then
            return true
        else
            return false
        endif
    else
        REM Pressure mode
        check error
            val(float_target, target_eb.value)
        on error
            _clearexception()
            float_target=-1
        enderr

        if float_target>=50 and float_target<=pmax[pump_idx] and float_rate>=1 and float_rate<=200 then
            return true
        else
            return false
        endif
    endif

    return true
endfunc

func start_pump(pump_idx as integer, ddl_mode as dropdownlistv2, eb_target as editboxv2, eb_rate as editboxv2)
    REM Start button pressed. Pump_idx2: 0 = a side, 1 = b side.
    REM Send command to sart pump operation.
    dim start_pump_rate, start_pump_target_f as float
    dim labelToStart, start_pump_target_s as string
    if ddl_mode.selected==0 then
        REM Flow mode start pressed
        check error
            val(start_pump_rate, eb_rate.Value)
            if eb_target.value=="Inf" then
                start_pump_target_s="2000000000"
            else
                val(start_pump_target_f, eb_target.Value)
                start_pump_target_s=str(start_pump_target_f)
            endif
        on error
            _clearexception()
            warning_sound()
        enderr
        start_pump_rate=(-1*start_pump_rate)
        if pump_idx==0 then
            gmccommand.command="TarVolA="+start_pump_target_s+";PmpRateA="+str(start_pump_rate)
            labelToStart="STRTFLA"
        elseif pump_idx==1 then
            gmccommand.command="TarVolB="+start_pump_target_s+";PmpRateB="+str(start_pump_rate)
            labelToStart="STRTFLB"
        endif
        gmccommand.executeCommand=true
        startthread(labelToStart,0)

    else
        REM Pressure mode start
        check error
            val(start_pump_rate, eb_rate.Value)
            val(start_pump_target_f, eb_target.Value)
            start_pump_target_s=str(start_pump_target_f)
        on error
            _clearexception()
            warning_sound()
        enderr
        if pump_idx==0 then
            LabelToStart="STRTPRA"
            gmccommand.command="TarPresA="+start_pump_target_s+";RampRatA="+str(start_pump_rate)+";RampingA=1"
        else
            LabelToStart="STRTPRB"
            gmccommand.command="TarPresB="+start_pump_target_s+";RampRatB="+str(start_pump_rate)+";RampingB=1"
        endif
        gmccommand.executeCommand=true
        StartThread(LabelToStart,0)
    endif

endfunc

