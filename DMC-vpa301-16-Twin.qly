func TicksToDispString(ticks as float) returns string
    return SecondsToDispString(integer(ticks/32768.0))
endfunc

func SecondsToDispString(sec as integer) returns string
    dim hour as integer
    dim min as integer
    dim hStr, mStr, sStr as string

    hour := sec / 3600
    sec := sec mod 3600
    min := sec / 60
    sec := sec mod 60
    hStr := str(hour)
    while (len(hStr) < 2) do
        hStr := "0" + hStr
    loop
    mStr := str(min)
    while (len(mStr) < 2) do
        mStr := "0" + mStr
    loop
    sStr := str(sec)
    while (len(sStr) < 2) do
        sStr := "0" + sStr
    loop
    return hStr + ":" + mStr + ":" + sStr
endfunc

