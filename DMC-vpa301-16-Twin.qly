    if flow_pressed==start then
        but_startflow.Value="STOP"
        but_UpdateFlow.enabled=false
        lbl_Flow_Instructions.value="Flowing at constant rate."
        set_case=1
    elseif flow_pressed==stop then
        lbl_Flow_Instructions.value="Waiting for cylinders to stop."

        set_case=0
    elseif flow_pressed==cycle then
        but_UpdateFlow.enabled=false
        but_startflow.Value="STOP"
