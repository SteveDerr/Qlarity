func StartThread(ListLabel as string, attempt as integer) returns boolean
    REM Waits to see if thead one is open. If not attempts to send command again in 200 MS. Returns false and throws error if failed on 5th attempt.
    if ThreadOneClear then
        gmccommand.command="XQ#"+ListLabel+",1"
        gmccommand.executeCommand=true
        'transmit(COM1, "XQ#"+ListLabel+",1",false)
        return true
    else
        if attempt==5 then
            handle_error(21)
            return false
        else
            delayms(200)
            StartThread(listlabel, attempt+1)
            return true
        endif
    endif
endfunc



