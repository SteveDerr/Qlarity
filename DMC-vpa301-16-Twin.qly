func determine_valve_color(pump_side as integer) returns integer
    REM Determines what is happening with the valves. valveopen is array of @OUT[x] signals.
    dim valve_idx as integer

    if pump_side==0 then
        valve_idx=0
    else
        valve_idx=2
    endif

    if valveopen[valve_idx+1]==true then
        REM Outlet is open show green
        return 20

    elseif valveopen[valve_idx]==true then
        REM Inlet is open, show yellow
        return 252

    else
        REM Both are closed, show red
        return 160
    endif

endfunc


