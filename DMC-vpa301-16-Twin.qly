Level 249587
#Visible True
#Lock False
init parent := daq_objects
init commobject := galilcomm

func ResponseReceived(response as string)
    dim start_idx[8], length[8], curpos, loopI as integer
    dim tmp_cycldones[2] as float
    dim strTime as string


    for loopi=0 to 7 step 1
        if loopi==0 then
            start_idx[loopi]=0
            length[loopi]=find(response,0,-1, " ")
            curpos=length[loopi]+1

        elseif loopi==7 then
            start_idx[loopi]=(curpos)
            length[loopi]=(len(response)-curpos)


        else
            start_idx[loopi]=(curpos)
            length[loopi]=(find(response,start_idx[loopi],-1, " ")-curpos)
            curpos=curpos+length[loopi]+1
        endif
    next

    check error
        val(FStates[0],left(response,length[0]))
        val(FStates[1],mid(response,start_idx[1],length[1]))
        val(PStates[0],mid(response,start_idx[2],length[2]))
        val(PStates[1],mid(response,start_idx[3],length[3]))
        val(InitDones[0],mid(response,start_idx[4],length[4]))
        val(InitDones[1],mid(response,start_idx[5],length[5]))
        val(tmp_CyclDones[0],mid(response,start_idx[6],length[6]))
        val(tmp_CyclDones[1],right(response,length[7]))
    on error
        Date_and_Time(strTime)
        error_log_eb.Append("\n\n"+strtime+" "+str(me)+"  Error reading twin cylinder variables. Response read as "+response, true)
        _clearexception()
    enderr

    if tmp_cycldones[0]==1 then
        cycldones[0]=true
    else
        cycldones[1]=false
    endif
    if tmp_cycldones[1]==1 then
        cycldones[1]=true
