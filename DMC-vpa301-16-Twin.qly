func Handle_Error(errorID as Integer)
    dim strTime as string
    Date_and_Time(strTime)
    error_log_eb.Append("\n\n"+strtime+" Error ID "+str(ErrorID), true)
    Error_Log_eb.append("\nCurrent Values - PcurA: " +str(pcur[0]) +", PcurB: " +str(pcur[1]) +", AveSpA: " + str(avesp[0])+", AveSpB: "+str(avesp[1])+", Pressure warning limit: " +eb_pmax_warn_a.Value +", Pressure Shutdown limit " + eb_pmax_a.value +", Flow Warning " + eb_flowrlim_warn_a.value +", Flow Shutdown " + eb_flowrlim_a.Value,true)
    pv_errorlog.Save()





REM Rem Buttons can only be set in Set_Error_Buttons
'    if errorID==14 then 'we must force them to restart the pump
'        but_Ignore_Error.enabled=false
'    endif
    if Hold_This_Error then'we won't handle any more errors
        return
    endif
    REM errors 12 and 13 are handled by Galil and the pump should already be stopped
    REM Always show this error.
    'if errorID>11 and errorID<14 then '12 and 13 are exceeded max press A and B
    '    If flow_mode_screen.enabled==true then  'only apply this error in flowmode
    '        return
    '    endif
    'endif
    if ErrorDisplayDisabled[0] and errorID==19 then
        return
        'Don't display if warnings disabled.
    endif
    if tmr_overPresAck.enabled and (errorID==12 or errorid==13) then
        REM don't display if still within 30 seconds of being acknowledged
        return
    endif
    if ErrorDisplayDisabled[0] and errorID >4 and errorID<8 then
        'do not update the error log or display message
    else
        debugText="Error ID "+str(ErrorID)+"\nCurrent Values - PcurA: " +str(PcurA) +", PcurB: " +str(PcurB) +", AveSpA: " + str(avesp[0])+", AveSpB: " + str(avesp[1])+", Pressure warning limit: " +eb_pmax_warn_a.Value +", Pressure Shutdown limit " + eb_pmax_a.value +", Flow Warning " + eb_flowrlim_warn_a.value +", Flow Shutdown " + eb_flowrlim_a.Value
        Write_To_Debug(debugText, "Global Function Handle_Error")

        Date_and_Time(strTime)
        Update_Error_Arrays(errorID,strTime)
        Init_Error_Popup_Screen(errorID)
    endif
endfunc



