Func Update_Maint_Hours()
    REM Function converts start time and stop time to floats, converts total runtime to hours then updates the maintenance hours.
    dim F_StartTime, F_StopTime as float

    check error
             Val(MaintHours, lbl_maint_hrs.value)
    on error
            _ClearException()
    enderr

    F_StartTime=StartTime
    F_StopTime=StopTime
    RunHours=((F_StopTime-F_StartTime)/3600)
    MaintHours=MaintHours+RunHours
    RunHours=0
    lbl_maint_hrs.value=_FormatFloat(MaintHours, 6,2,_FLOAT_NORMAL)
    PV_maintenance.Save()
endfunc


