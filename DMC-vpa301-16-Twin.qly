func check_errors()
    REM Check to see if we need to handle any errors.
    if not remote_control then
        REM Don't check the Galil errors if we are in remote control
        if newerr[0]==1 then
            gmccom_errmsgA.executeCommand=true
            gmccommand.command="NewErrA=0"
            gmccommand.executeCommand=true
        endif
        if newerr[1]==1 then
            gmccom_errmsgB.executeCommand=true
            gmccommand.command="NewErrB=0"
            gmccommand.executeCommand=true
        endif
    endif
    if not DAQrunning then
        handle_error(14)
    endif
    if pcur[0]>=pmaxwarn[0] then
        handle_error(5)
    endif
    if pcur[1]>=pmaxwarn[1] then
        handle_error(6)
    endif
    if avesp[0]>=flowRLimWarn[0] and not errordisplaydisabled[0] then
        handle_error(7)
    endif
    if avesp[1]>=FlowRLimWarn[1] and not errordisplaydisabled[1] then
        handle_error(7)
    endif
    if avesp[0]>=FloWRLim[0] and not flow_limit_disabled[0] then
        gmccommand.command="ReStpPA=1"
        gmccommand.executeCommand=true
        handle_error(10)
    endif
    if avesp[1]>=FlowRLim[1] and not flow_limit_disabled[1] then
        gmccommand.command="ReStpPB=1"
        gmccommand.executeCommand=true
        handle_error(10)
    endif
endfunc

